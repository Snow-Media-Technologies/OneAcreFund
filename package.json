{
  "name": "oaf-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "test": "mocha --recursive tests --timeout 10000",
    "dev": "nodemon app.js",
    "coverage": "nyc --reporter=html --reporter=text mocha tests --timeout=10000"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "lodash": "^4.17.15",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.2",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "imgur": "^0.3.1",
    "jest": "^25.1.0",
    "mocha": "^7.1.0",
    "mocha-eslint": "^6.0.0",
    "nyc": "^15.0.0",
    "puppeteer": "^2.1.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "semi": "warn",
      "eqeqeq": "warn",
      "curly": "warn",
      "no-bitwise": "warn"
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
